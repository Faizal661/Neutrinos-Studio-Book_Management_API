    "scripts": {
        "tsc": "tsc",
        "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "start": "npm run node-prod",
        "startserver": "set debug=* && nodemon --ext ts,mjs,js --import ./loadEnvs.mjs --import ./loader.mjs --inspect src/index.ts",
        "start-dev": "cross-env NEUTRINOS_APP_ENV=dev npm run startserver",
        "node-dev": "cross-env NEUTRINOS_APP_ENV=dev node --import ./loadEnvs.mjs ./build/index.js",
        "start-prod": "cross-env NEUTRINOS_APP_ENV=prod npm run startserver",
        "node-prod": "cross-env NEUTRINOS_APP_ENV=prod node --import ./loadEnvs.mjs ./build/index.js",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "pm2": "npm run build && pm2 start ecosystem.config.cjs",
        "node-prod-trace": "cross-env NEUTRINOS_APP_ENV=prod node --import ./loadEnvs.mjs --import './tracer.js' ./build/index.js"
    }
